{
  "name": "cashback",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@storybook/cli": "^6.0.21",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^7.1.2",
    "connected-react-router": "^6.8.0",
    "dayjs": "^1.8.35",
    "emotion-normalize": "^10.1.0",
    "emotion-theming": "^10.0.27",
    "history": "^5.0.0",
    "immer": "^7.0.9",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.8.3",
    "react-icons": "^3.11.0",
    "react-ink": "^6.4.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-toastify": "^6.0.8",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "tree-changes": "^0.5.1",
    "uuid": "^8.3.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "babel-eslint": "^10.1.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-redux-saga": "^1.2.0",
    "eslint-plugin-testing-library": "^3.8.0",
    "husky": "^4.3.0",
    "jest-chain": "^1.1.5",
    "jest-emotion": "^10.0.32",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "stylelint-config-prettier": "^8.0.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js --ext .jsx src",
    "lint:fix": "yarn lint --fix",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "test": "react-scripts test --modulePaths=src",
    "test:coverage": "yarn test --coverage --watchAll=false --bail"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "hooks": {
    "post-merge": "yarn install",
    "pre-commit": "lint-staged"
  },
  "jest": {
    "collectCoverageFrom": [
      "!/node_modules/",
      "!src/config/*",
      "!src/components/**/index.(js|jsx|ts|tsx)",
      "!src/containers/**/index.(js|jsx|ts|tsx)",
      "src/**/*.(js|jsx|ts|tsx)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
